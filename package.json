{
  "name": "figma-import-react-icons",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Figma plugin to import icon sets from the react-icons library.",
  "author": "Darcy Glennen <darcy@portable.com.au>",
  "contributors": [
    "Darcy Glennen <darcy@portable.com.au>"
  ],
  "main": "dist/code.js",
  "scripts": {
    "build": "webpack --mode=production",
    "build:watch": "webpack --mode=production --watch --info-verbosity verbose",
    "build:analyze": "ANALYZE=true yarn build",
    "lint": "eslint . --fix --ext .js,.ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn typecheck & lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-icons": "^3.11.0"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.17.1",
    "@types/node": "^14.11.10",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.5.0",
    "css-loader": "^5.0.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "html-webpack-inline-source-plugin": "1.0.0-beta.2",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.3",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.6",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.1.0"
  }
}
