{
  "name": "figma-import-react-icons",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Figma plugin to import icon sets from the react-icons library.",
  "author": "Darcy Glennen <darcy@portable.com.au>",
  "contributors": [
    "Darcy Glennen <darcy@portable.com.au>"
  ],
  "main": "dist/code.js",
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "pre-push": "yarn typecheck",
    "build": "webpack --mode=production",
    "build:watch": "webpack --mode=production --watch",
    "build:analyze": "ANALYZE=true yarn build",
    "verify": "concurrently yarn:lint yarn:typecheck",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.36.0",
    "@types/node": "^16.10.2",
    "@types/react": "^17.0.25",
    "@types/react-dom": "^17.0.9",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "concurrently": "^6.3.0",
    "css-loader": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "webpack": "^5.55.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
