{
  "name": "figma-import-react-icons",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Figma plugin to import icon sets from the react-icons library.",
  "author": "Darcy Glennen <darcy@portable.com.au>",
  "contributors": [
    "Darcy Glennen <darcy@portable.com.au>"
  ],
  "main": "dist/code.js",
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "pre-push": "yarn typecheck",
    "build": "webpack --mode=production",
    "build:watch": "webpack --mode=production --watch",
    "build:analyze": "ANALYZE=true yarn build",
    "verify": "concurrently --group yarn:lint yarn:typecheck",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.47.0",
    "@types/node": "^18.7.16",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "concurrently": "^7.2.1",
    "css-loader": "^6.7.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "prettier": "^2.7.0",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
